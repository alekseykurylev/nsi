generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("PRISMA_DATABASE_URL")
}

model Okpd2 {
  id         Int     @id @default(autoincrement())
  code       String  @unique
  parentCode String?
  name       String
  actual     Boolean @default(true)

  @@map("okpd2")
}
