generator client {
  provider        = "prisma-client-js"
  output          = "../src/generated/prisma"
  previewFeatures = ["typedSql", "fullTextSearchPostgres"]
}

datasource db {
  provider = "postgresql"
  url      = env("PRISMA_DATABASE_URL")
}

model Okpd2 {
  id         Int     @id
  parentId   Int?
  code       String
  parentCode String?
  name       String
  actual     Boolean

  @@map("okpd2")
}
